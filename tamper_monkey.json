{
  "@name": "youtube内容屏蔽器",
  "alias": "youtube_content_shield",
  "@namespace": "http://tampermonkey.net/",
  "@license": "GPL-3.0",
  "@version": "1.0",
  "@author": "byhgz",
  "@description": "对油管站内内容屏蔽处理",
  "@icon": "https://static.hdslb.com/images/favicon.ico",
  "noFrames": true,
  "@run-at": "document-start",
  "@grant": [
    "GM_setValue",
    "GM_getValue",
    "GM_deleteValue",
    "GM_addStyle",
    "GM_unregisterMenuCommand",
    "GM_registerMenuCommand",
    "GM_openInTab"
  ],
  "@match": [
    "*://www.youtube.com/*"
  ],
  "@exclude": [
    "http://localhost:3002/"
  ],
  "@require": [
    "https://unpkg.com/vue@2.7.16/dist/vue.min.js",
    "https://unpkg.com/element-ui@2.15.14/lib/index.js"
  ]
}

